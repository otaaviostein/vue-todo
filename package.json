{
  "name": "vue-todo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:unit": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.22.10",
    "@types/random-words": "^1.1.2",
    "@types/uuid": "^9.0.2",
    "@unocss/reset": "^0.55.2",
    "animate.css": "^4.1.1",
    "date-fns": "^2.30.0",
    "pinia": "^2.1.6",
    "random-words": "^1.1.2",
    "uuid": "^9.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.5",
    "babel-jest": "^29.6.4",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "sass": "^1.50.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.5.4",
    "unocss": "^0.55.2",
    "vite": "^2.9.0",
    "vue-tsc": "^1.8.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@pinia)"
    ],
    "testEnvironmentOptions": {
      "customExportConditions": ["node", "node-addons"]
   },
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
